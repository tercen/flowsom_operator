{
  "name": "FlowSOM",
  "description": "Clusters flow cytometry data points using a graph-based algorithm.",
  "tags": [
    "flow cytometry",
    "clustering"
  ],
  "container": "tercen/flowsom_operator:1.6.0",
  "authors": [
    "tercen"
  ],
  "urls": [
    "https://github.com/tercen/flowsom_operator"
  ],
  "properties": [
    {
      "kind": "StringProperty",
      "name": "nclust",
      "defaultValue": "NULL",
      "description": "Number of clusters to infer"
    },
    {
      "kind": "StringProperty",
      "name": "maxMeta",
      "defaultValue": "NULL",
      "description": "Maximal number of clusters (if specified, nclust is ignored). If both nclust and maxMeta are NULL, maxMeta is set to 10."
    },
    {
      "kind": "DoubleProperty",
      "name": "seed",
      "defaultValue": -1,
      "description": "Seed for random number generation. if less than 0, set a Random seed"
    },
    {
      "kind": "DoubleProperty",
      "name": "xdim",
      "defaultValue": 10,
      "description": "Size of x dim for the SOM grid"
    },
    {
      "kind": "DoubleProperty",
      "name": "ydim",
      "defaultValue": 10,
      "description": "Size of y dim for the SOM grid"
    },
    {
      "kind": "DoubleProperty",
      "name": "rlen",
      "defaultValue": 10,
      "description": "Size of rlen"
    },
    {
      "kind": "DoubleProperty",
      "name": "mst",
      "defaultValue": 1,
      "description": "Amount of training"
    },
    {
      "kind": "DoubleProperty",
      "name": "alpha_1",
      "defaultValue": 0.05,
      "description": "Alpha1 setting"
    },
    {
      "kind": "DoubleProperty",
      "name": "alpha_2",
      "defaultValue": 0.01,
      "description": "Alpha2 setting"
    },
    {
      "kind": "DoubleProperty",
      "name": "distf",
      "defaultValue": 2,
      "description": "Distance setting"
    }
  ],
  "operatorSpec": {
    "kind": "OperatorSpec",
    "ontologyUri": "https://tercen.com/_ontology/tercen",
    "ontologyVersion": "0.0.1",
    "inputSpecs": [
      {
        "kind": "CrosstabSpec",
        "metaFactors": [
          {
            "kind": "MetaFactor",
            "name": "Variable",
            "type": "",
            "description": "Variable identifiers (channels, markers)",
            "ontologyMapping": "variable",
            "crosstabMapping": "row",
            "cardinality": "1..n",
            "factors": []
          },
          {
            "kind": "MetaFactor",
            "name": "Observation",
            "type": "",
            "description": "Observation identifiers (cells)",
            "ontologyMapping": "observation",
            "crosstabMapping": "column",
            "cardinality": "1..n",
            "factors": []
          }
        ],
        "axis": [
          {
            "kind": "AxisSpec",
            "metaFactors": [
              {
                "kind": "MetaFactor",
                "name": "Measurement",
                "type": "numeric",
                "description": "Measurement value of the signal",
                "ontologyMapping": "measurement",
                "crosstabMapping": "y",
                "cardinality": "1",
                "factors": []
              }
            ]
          }
        ]
      }
    ]
  }
}