{
  "name": "FlowSOM",
  "description": "Flow Cytometry Self Organising Maps: An algorithm that clusters cells based on chosen channels and maps them into populations.",
  "tags": ["flow cytometry"],
  "authors": ["tercen"],
  "urls": ["https://github.com/tercen/flowsom_operator"],
  "container": "tercen/flowsom:0.1.9",
  "properties": [
   {
    "kind": "StringProperty",
    "name": "nclust",
    "defaultValue": "NULL",
    "description": "number of clusters"
  },
  {
    "kind": "DoubleProperty",
    "name": "maxMeta",
    "defaultValue": 10,
    "description": "Maximal number of clusters"
  },
  {
    "kind": "StringProperty",
    "name": "seed",
    "defaultValue": "NULL",
    "description": "random seed"
  },
  {
    "kind": "DoubleProperty",
    "name": "xdim",
    "defaultValue": 10,
    "description": "size of x dim for the som grid"
  },
   {
    "kind": "DoubleProperty",
    "name": "ydim",
    "defaultValue": 10,
    "description": "size of y dim for the som grid"
  },
  {
    "kind": "DoubleProperty",
    "name": "rlen",
    "defaultValue": 10,
    "description": "size of rlen"
  },
   {
    "kind": "DoubleProperty",
    "name": "mst",
    "defaultValue": 1,
    "description": "amount of training"
  },
   {
    "kind": "DoubleProperty",
    "name": "alpha_1",
    "defaultValue": 0.05,
    "description": "alpha setting"
  },
   {
    "kind": "DoubleProperty",
    "name": "alpha_2",
    "defaultValue": 0.01,
    "description": "alpha setting"
  },
 {
    "kind": "DoubleProperty",
    "name": "distf",
    "defaultValue": 2,
    "description": "distance setting"
  }
  ]
}


